using System;

class Program {
  public static void Main (string[] args) {
    
    double x = 3 * (4 + Math.Pow(7, 2)) + 3 / 1 - 45;
    Console.WriteLine(x);

    double A = (2+ Math.Pow(8, 2)) + 9 / 3 - 1;
    Console.WriteLine(A);

    bool afirm1 = (Math.Pow(5, 2)) > (16.0 / 3);
    Console.WriteLine(afirm1);

    bool afirm2 = (Math.Pow(2, 2)) > (3 + 2);
    Console.WriteLine(afirm2);

    bool vdd = (1 * 3 + 4) > (3 + 3) && (3 + 1) < (5 * 6 + 3);
    Console.WriteLine(vdd);
  }
}
